{
  "expo": {
    "name": "ClassBridge Mobile",
    "slug": "classbridge-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "classbridge",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.classbridge.mobile"
    },
    "android": {
      "package": "com.classbridge.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
