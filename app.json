{
  "expo": {
    "name": "Krishnaveni Talent School",
    "slug": "classbridge-mobile",
    "version": "1.0.0",
    "orientation": "default",
    "scheme": "krishnaveni",
    "icon": "./assets/images/Image.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.krishnaveni.talentschool",
      "icon": "./assets/images/Image.png",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSIncludesSubdomains": false
            }
          }
        },
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "package": "com.krishnaveni.talentschool",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Image.png",
        "backgroundColor": "#FF6B35"
      },
      "icon": "./assets/images/Image.png",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "screenOrientation": "unspecified"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ef0da41-8d21-45f7-ade7-fca22e8e5436"
      }
    }
  }
}
